{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.34",
    "@vuelidate/validators": "^2.0.0-alpha.26",
    "@vueuse/core": "^7.4.0",
    "graphql": "^16.2.0",
    "pinia": "2.0.9",
    "villus": "^1.0.1",
    "vue": "^3.2.26",
    "vue-cookie-next": "^1.3.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.16.0",
    "@iconify/json": "^1.1.457",
    "@pinia/testing": "0.0.9",
    "@vitejs/plugin-vue": "2.0.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "c8": "^7.11.0",
    "eslint": "8.6.0",
    "happy-dom": "^2.31.1",
    "http-server": "14.1.0",
    "jest": "^27.5.1",
    "msw": "^0.36.8",
    "npm-check": "^5.9.2",
    "sass": "1.48.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "0.5.10",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.9",
    "vite": "2.7.12",
    "vite-plugin-istanbul": "^2.3.0",
    "vite-plugin-pages": "0.20.0",
    "vite-plugin-windicss": "1.6.2",
    "vitest": "^0.3.2",
    "vue-router-mock": "^0.1.7",
    "vue-tsc": "0.30.2",
    "windicss": "3.4.2"
  }
}
