{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.2",
    "@formkit/vue": "^1.0.0-beta.2",
    "@vueuse/core": "8.4.0",
    "graphql": "16.4.0",
    "pinia": "2.0.13",
    "villus": "1.1.0",
    "vue": "3.2.33",
    "vue-cookie-next": "^1.3.0",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.23.0",
    "@iconify/json": "2.1.35",
    "@pinia/testing": "0.0.11",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "c8": "7.11.2",
    "eslint": "8.14.0",
    "http-server": "14.1.0",
    "npm-check": "^5.9.2",
    "sass": "1.51.0",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.7.1",
    "unplugin-icons": "0.14.3",
    "unplugin-vue-components": "0.19.3",
    "vite": "2.9.7",
    "vite-plugin-pages": "0.23.0",
    "vite-plugin-windicss": "1.8.4",
    "vitest": "0.10.2",
    "vue-router-mock": "^0.1.7",
    "vue-tsc": "0.34.11",
    "windicss": "3.5.1"
  }
}
