{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.2",
    "@formkit/vue": "^1.0.0-beta.2",
    "@vueuse/core": "8.5.0",
    "graphql": "16.6.0",
    "pinia": "2.0.22",
    "villus": "1.1.0",
    "vue": "3.2.39",
    "vue-cookie-next": "^1.3.0",
    "vue-router": "4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.26.3",
    "@iconify/json": "2.1.107",
    "@pinia/testing": "0.0.14",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/test-utils": "2.0.2",
    "c8": "7.12.0",
    "eslint": "8.23.1",
    "happy-dom": "^3.2.0",
    "http-server": "14.1.1",
    "npm-check": "^5.9.2",
    "sass": "1.54.9",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.11.2",
    "unplugin-icons": "0.14.9",
    "unplugin-vue-components": "0.22.7",
    "vite": "2.9.9",
    "vite-plugin-pages": "0.26.0",
    "vite-plugin-windicss": "1.8.8",
    "vitest": "0.23.2",
    "vue-router-mock": "0.1.9",
    "vue-tsc": "0.40.13",
    "windicss": "3.5.6"
  }
}
