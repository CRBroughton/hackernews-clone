{
  "name": "my-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "pnpm:check": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "type:check": "vue-tsc --noEmit ",
    "lint:check": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "serve": "vite preview",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.2",
    "@formkit/vue": "^1.0.0-beta.2",
    "@vueuse/core": "9.6.0",
    "graphql": "16.6.0",
    "pinia": "2.0.26",
    "villus": "1.1.0",
    "vue": "3.2.45",
    "vue-cookie-next": "^1.3.0",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.31.0",
    "@iconify/json": "2.1.143",
    "@pinia/testing": "0.0.14",
    "@vitejs/plugin-vue": "3.2.0",
    "@vue/test-utils": "2.2.4",
    "c8": "7.12.0",
    "eslint": "8.28.0",
    "happy-dom": "7.7.0",
    "http-server": "14.1.1",
    "npm-check": "6.0.1",
    "sass": "1.56.1",
    "typescript": "4.9.3",
    "unplugin-auto-import": "0.11.5",
    "unplugin-icons": "0.14.14",
    "unplugin-vue-components": "0.22.11",
    "vite": "3.2.4",
    "vite-plugin-pages": "0.27.1",
    "vite-plugin-windicss": "1.8.8",
    "vitest": "0.25.3",
    "vue-router-mock": "0.1.9",
    "vue-tsc": "1.0.9",
    "windicss": "3.5.6"
  }
}
